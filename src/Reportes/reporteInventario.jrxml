	<parameter name="sucursal" class="java.lang.String"/>
	<parameter name="tipoProd" class="java.lang.String"/>
	<queryString>
		<![CDATA[select tipo_prod as TipoProd,nom_prod as Producto,nom_pre as Presentacion,stock_prod as Stock,
date_format(fechE_prod,'%d-%m-%Y') as FechaE,date_format(fechC_prod,'%d-%m-%Y') as FechaC 
FROM almacen,pedidos,productosproveedor as prodProv,proveedor,producto,presentacion
where almacen.cod_ped = pedidos.cod_ped 
and suc_ped = $P{sucursal}
and pedidos.cod_prodProv = prodProv.cod_ProdProv 
and prodProv.cod_prov = proveedor.cod_prov
and prodProv.cod_prod = producto.cod_prod
and producto.cod_pre = presentacion.cod_pre
and tipo_prod like $P{tipoProd}
order by tipo_prod]]>
	</queryString>
