	<parameter name="Vendedor" class="java.lang.String"/>
	<parameter name="fechaDel" class="java.sql.Date"/>
	<parameter name="fechaAl" class="java.sql.Date"/>
	<queryString>
		<![CDATA[SELECT
     cod_fac AS nroFactura,
     format(sbtotal_fac,2) AS subtotal,
     format(sbtotal_fac*0.13,2) AS IVA,
     format(total_fac,2) AS total,
     date_format(fech_fac,'%d-%m-%Y') AS fecha,
     concat(nom_cli,' ',ape_cli) AS cliente,
     concat(nom_emp,' ',ape_emp) AS empleado
FROM
     cliente,empleados,factura
WHERE
     ci_vend = ci_emp
     and
     factura.cod_cli = cliente.ci_cli
     and
     concat(nom_emp,' ',ape_emp) like $P{Vendedor}
     and
     fech_fac >=  $P{fechaDel}
     and 
     fech_fac <= $P{fechaAl}
     order by 
     fech_fac]]>
	</queryString>
