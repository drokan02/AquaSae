		<![CDATA[select 
	tipo_prod as Tipo,nom_prod as Producto,nom_pre as Presentacion,
	prec_prod as 'Precio/u',stock_ped as Cantidad, cost_ped as Costo,
	date_format(fech_ped,'%d-%m-%Y') as Fecha, date_format(fechEn_ped,'%d-%m-%Y') as FechaEn
from 
	pedidos,productosproveedor as prodProv,producto,presentacion
where
	pedidos.cod_prodProv = prodProv.cod_ProdProv
	and prodProv.cod_prod = producto.cod_prod
	and producto.cod_pre = presentacion.cod_pre
	and month(fech_ped) = $P{mes}
	and year(fech_ped) = $P{anio}]]>
