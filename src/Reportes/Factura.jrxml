	<parameter name="codigoFac" class="java.lang.String"/>
	<queryString>
		<![CDATA[select  factura.cod_fac as codigoFac,
concat(nom_cli,' ',ape_cli) as momCli,
cod_cli as ci,
dire_cli as direccionCli,
telf_cli as telefonoCli,
format(sbtotal_fac,2) as subtotal,
format(total_fac,2) as total,
format((total_fac - sbtotal_fac),2) as Iva,
date_format(fech_fac,'%d/%m/%Y') as fecha,
nom_prod as producto,
nom_pre as presentacion,
format((prec_prod+prec_prod*0.15),2) as precio ,
cant_prod as cantidad,
format(cost_vent,2) as costo 
from factura,cliente,venta,producto,presentacion 
where $P{codigoFac} = factura.cod_fac 
and factura.cod_fac = venta.cod_fac 
and factura.cod_cli = cliente.ci_cli and  venta.cod_prod = producto.cod_prod
and producto.cod_pre = presentacion.cod_pre]]>
	</queryString>
