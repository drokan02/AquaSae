	<queryString>
		<![CDATA[SELECT
	(SELECT format(sum(cost_vent),2) 
	 FROM factura,venta
	 WHERE 
            venta.cod_fac = factura.cod_fac 
            AND date_format(fech_fac,'%d-%m-%Y') LIKE $P{tipoReport})as costoTotal,
	nom_prod as Producto,nom_pre as Presentacion,
	tipo_prod as tipProducto ,format(prec_prod,2) as Precio, 
	sum(cant_prod) as cantidadVendida ,format(sum(cost_vent),2) as Ingreso
FROM 
            factura,producto,venta,presentacion
WHERE 
             venta.cod_fac = factura.cod_fac 
             AND venta.cod_prod = producto.cod_prod
             AND producto.cod_pre = presentacion.cod_pre 
             AND date_format(fech_fac,'%d-%m-%Y') LIKE $P{tipoReport}
             GROUP BY nom_prod]]>
	</queryString>
